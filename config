[bar/vicyos-mainbar]

;MONITOR=$(polybar -m|tail -1|sed -e 's/:.*$//g')
;monitor = DisplayPort-0
;monitor-fallback = HDMI-0

width = 100%
height = 30
offset-x = 

; The space between the Polybar and the top edge of the monitor. (It gives a nice looking).
offset-y = :4
radius = 10

fixed-center = true
bottom = false
separator = 

background = ${colors.background}
foreground = ${colors.foreground}

line-size = 2
line-color = #f00

;wm-restack = bspwm
override-redirect = false

; Enable support for inter-process messaging
; See the Messaging wiki page for more details.
enable-ipc = true

;border-size = 2
;border-top-size = 3

; The space between the Polybar and the windows. (It gives a nice looking).
border-bottom-size = :6

border-left-size  = 16
border-right-size = 16
border-color = #00000000

padding-left = 1
padding-right = 2

module-margin-left = 1
module-margin-right = 1

;https://github.com/jaagr/polybar/wiki/Fonts
font-0 = "Bitstream Vera Sans:style=Roman:size=10;2"
font-1 = "FontAwesome:size=11;3"
font-2 = "Noto Sans Mono:size=15;4"
font-3 = "Bitstream Vera Sans:style=Roman:size=15;4"

; Import devicons/icomoon font to enable Chromium icon
font-4 = "icomoon:size=15;3"

; Import icomoon-feather to enable checknetwork icon
font-5 = "icomoon\-feather:size=13;3"

; Import Ubuntu-C to test icon
font-6 = "Ubuntu Condensed:size=15;3"

;Optional modules
; startmenu urxvt chromium thunar date

modules-left = cpu-graph  ewmh  xwindow
modules-center = xfcestart 
modules-right =  checknetwork rofi pavucontrol updates volume temperature1 memory1 weather date1 clock  powermenu 
;

;tray-detached = false
;tray-offset-x = 0
;tray-offset-y = 0
;tray-padding = 0
;tray-maxsize = 24
;tray-scale = 1
;tray-position = false
tray-background = ${colors.background}
;tray-foreground = ${colors.foreground}

;scroll-up = herbstclient use_index -1
;scroll-down = herbstclient use_index +1



